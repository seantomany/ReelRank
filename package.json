{
  "name": "reelrank",
  "private": true,
  "scripts": {
    "dev:api": "turbo dev --filter=@reelrank/api",
    "dev:mobile": "turbo dev --filter=@reelrank/mobile",
    "build": "turbo build",
    "lint": "turbo lint",
    "test": "turbo test",
    "db:push": "pnpm --filter @reelrank/api db:push",
    "db:migrate": "pnpm --filter @reelrank/api db:migrate",
    "db:generate": "pnpm --filter @reelrank/api db:generate"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@9.15.0"
}
